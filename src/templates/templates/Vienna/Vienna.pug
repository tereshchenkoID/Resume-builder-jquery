doctype html
html(lang='en')
  head
    title Vienna
    meta(charset="utf-8")
    meta(http-equiv="X-UA-Compatible" content="IE=Edge")
    meta(name="viewport" content="width=device-width, initial-scale=1")

    link(rel="stylesheet" href="./Vienna/Vienna.css")
  body
    .page.page--vienna#vienna
      .t-header
        .t-container.t-container--accent
          .t-left
            .t-photo#t-photo
          .t-right
            .t-wrapper
              h2.t-h2
                span#t-first_name
                span#t-last_name
              h6.t-h6#t-work_type
              .t-wrapper
                .t-item
                  span#t-phone
                  span.t-divider
                  span(style="text-decoration: underline")#t-email
      .t-container
        .t-left
          .t-wrapper
            h6.t-h6#t-section-0
            .t-item#t-address
            .t-item#t-postal_code
            .t-item#t-city
            .t-item#t-country
            .t-item#t-date_birth
            .t-item#t-place_birth
            .t-item#t-nationality
            .t-item#t-driving_license
        .t-right
          .t-qr-code#t-qr-code
          .t-wrapper
            h6.t-h6#t-section-1
            .t-item.t-item--transparent#t-profile
